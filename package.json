{
  "name": "hahoorequest",
  "version": "0.1.1",
  "description": "A HTTP request abstract library",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-core/register test/*.js",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run test && npm run build",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/hahoorequest.git"
  },
  "keywords": [
    "http",
    "request",
    "abstract",
    "superagent",
    "fetch"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/hahoorequest/issues"
  },
  "homepage": "https://github.com/hahoocn/hahoorequest#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "whatwg-fetch": "^1.0.0"
  }
}
